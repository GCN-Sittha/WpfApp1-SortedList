<Window x:Class="WpfApp1_SortedList.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1_SortedList"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="keyTxt" HorizontalAlignment="Left" Height="79" Margin="82,66,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="238"/>
        <TextBox x:Name="valueTxt" HorizontalAlignment="Left" Height="79" Margin="82,0,0,0" TextWrapping="Wrap" VerticalAlignment="Center" Width="238"/>
        <Button x:Name="btnAdd" Content="Add" HorizontalAlignment="Left" Height="191" Margin="371,66,0,0" VerticalAlignment="Top" Width="272" Click="btnAdd_Click"/>

    </Grid>
</Window>
